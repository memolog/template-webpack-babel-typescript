{
  "name": "template-webpack-babel-typescript",
  "version": "1.0.0",
  "description": "Template to use webpack, babel, typescript (and React)",
  "main": "index.js",
  "scripts": {
    "test": "jest --config=./src/browser/jest.config.json && jest --config=./src/node/jest.config.json",
    "build": "rm -rf ./public && webpack --config ./src/browser/webpack.config.js && tsc -p ./src/node/tsconfig.json && cp ./src/browser/index.html ./public/browser/"
  },
  "keywords": [
    "webpack",
    "babel",
    "typescript"
  ],
  "author": "Yutaka Yamaguchi",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.1",
    "autoprefixer": "^9.6.5",
    "babel": "^6.23.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.3.2",
    "css-loader": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "whatwg-fetch": "^3.0.0"
  }
}
